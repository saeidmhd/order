                    android:text="@string/str_daily_tax_charge" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/report_item_min_height"
                android:gravity="center_vertical"
                android:orientation="horizontal"
                android:padding="@dimen/NormalSize">

                <com.mahak.order.widget.FontTextView
                    android:id="@+id/tvDailyPreFinalSum"
                    android:layout_width="0dip"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="left"
                    android:text="@string/sample_number" />

                <com.mahak.order.widget.FontTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/str_daily_final_sum" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/report_item_min_height"
                android:background="@color/forms_alter_row"
                android:gravity="center_vertical"
                android:orientation="horizontal"
                android:padding="@dimen/NormalSize">

                <com.mahak.order.widget.FontTextView
                    android:id="@+id/tvDailyPreCountOfProduct"
                    android:layout_width="0dip"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
