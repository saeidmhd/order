                <com.mahak.order.widget.FontTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/str_daily_count_of_products" />

            </LinearLayout>
        </LinearLayout>

        <LinearLayout
